        <td>
            <a href="/admin/img/edit?page=<%=page%>&id=<%=list[x]._id%>" class="layui-btn layui-btn-small layui-btn-normal">查看&修改</a>
            <a href="/admin/img/remove?id=<%=list[x]._id%>" class="layui-btn layui-btn-small layui-btn-danger">删除</a>
