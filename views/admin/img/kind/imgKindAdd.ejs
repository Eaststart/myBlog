<% include ../../common/header.ejs %>
<body class="layui-layout-body">
<div class="layui-layout layui-layout-admin">
    <% include ../../common/nav.ejs %>
    <% include ../common/leftSide.ejs %>
    <!-- 内容主体区域 -->
    <div class="layui-body">
        <!-- 内容主体区域 -->
        <div style="padding: 15px;">
            <form class="layui-form" action="/admin/imgKind/imgDoAdd" method="post" style="max-width:400px;" enctype="multipart/form-data">
                <div class="layui-form-item">
                    <label class="layui-form-label">类名</label>
                    <div class="layui-input-block">
                        <input type="text" name="name" required lay-verify="required" placeholder="请输入类名"
                               autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-form-item">
                    <label class="layui-form-label">创建时间</label>
                    <div class="layui-input-block">
                        <input type="text"  class="layui-input"  id="sendTime" required lay-verify="required" name="foundTime">
                    </div>
                </div>

                <div class="layui-form-item">
                    <div class="layui-input-block">
                        <button class="layui-btn" lay-submit lay-filter="formDemo">立即提交</button>
                        <button type="reset" class="layui-btn layui-btn-primary">重置</button>
                    </div>
                </div>
            </form>
        </div>
    </div>

    <% include ../../common/cor.ejs %>
</div>
</body>
</html>
<% include ../../common/footer.ejs %>